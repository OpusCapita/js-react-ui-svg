{
  "name": "opuscapita-react-ui-svg",
  "version": "1.0.2",
  "description": "React components for SVG (Scalable Vector Graphic).",
  "scripts": {
    "npm-build": "rimraf lib && babel --copy-files --no-babelrc --presets es2015,stage-0,react --plugins transform-decorators-legacy --ignore __tests__,*.spec.js,demo src/client/components --out-dir lib",
    "link-mode": "webpack --watch --config ./webpack.link.config.js",
    "lint": "eslint src/client",
    "start": "nodemon --watch www www/index --exec babel-node --presets es2015,stage-0,react --plugins transform-decorators-legacy",
    "npm-publish": "npm run npm-build && npm-publish",
    "publish-snapshot": "npm run npm-publish",
    "publish-release": "npm run npm-publish -- --release"
  },
  "author": {
    "name": "Kirill Volkovich",
    "email": "volkovich@scand.com"
  },
  "main": "lib/index.js",
  "files": [
    "lib",
    "README.md"
  ],
  "license": "Apache-2.0",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/OpusCapitaBES/js-react-ui-svg"
  },
  "peerDependencies": {
    "react": "0.14.8",
    "react-dom": "0.14.8"
  },
  "devDependencies": {
    "eslint-config-opuscapita": "1.0.3",
    "opuscapita-npm-scripts": "1.0.5",
    "opuscapita-showroom-client": "1.0.10",
    "opuscapita-showroom-server": "1.0.10",
    "opuscapita-ui-svg-icons": "1.0.3",
    "autoprefixer": "6.4.1",
    "babel-cli": "6.5.1",
    "babel-core": "6.4.0",
    "babel-eslint": "7.1.1",
    "babel-loader": "6.2.1",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-object-assign": "6.8.0",
    "babel-preset-es2015": "6.3.13",
    "babel-preset-react": "6.3.13",
    "babel-preset-stage-0": "6.3.13",
    "babel-register": "6.5.2",
    "babel-standalone": "6.14.0",
    "compression": "1.6.2",
    "cross-env": "1.0.7",
    "css-loader": "0.21.0",
    "eslint": "3.11.1",
    "eslint-plugin-react": "6.7.1",
    "express": "4.13.4",
    "file-loader": "0.9.0",
    "fs-extra": "1.0.0",
    "fuzzysearch": "1.0.3",
    "globule": "1.1.0",
    "json-loader": "0.5.4",
    "less": "2.3.1",
    "less-loader": "2.2.2",
    "lodash": "4.16.6",
    "nodemon": "1.9.1",
    "postcss-loader": "0.13.0",
    "postcss-modules": "0.5.2",
    "raw-loader": "0.5.1",
    "rimraf": "2.5.2",
    "source-map-loader": "0.1.5",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "1.12.2",
    "webpack-bundle-analyzer": "1.5.0",
    "webpack-dev-middleware": "1.5.1",
    "write-file-webpack-plugin": "3.3.0"
  }
}
